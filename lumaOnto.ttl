@prefix : <http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#> .

<http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#alleviatesSymptom
:alleviatesSymptom rdf:type owl:ObjectProperty ;
                   rdfs:domain :CopingStrategy ,
                               :Treatment ;
                   rdfs:range [ rdf:type owl:Restriction ;
                                owl:onProperty :alleviatesSymptom ;
                                owl:someValuesFrom :Symptom
                              ] .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#canInitiate
:canInitiate rdf:type owl:ObjectProperty ;
             rdfs:domain :Trigger ;
             rdfs:range [ rdf:type owl:Restriction ;
                          owl:onProperty :canInitiate ;
                          owl:someValuesFrom :MentalHealthCondition
                        ] .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#causeSymptom
:causeSymptom rdf:type owl:ObjectProperty ;
              rdfs:domain :Trigger ;
              rdfs:range [ rdf:type owl:Restriction ;
                           owl:onProperty :causeSymptom ;
                           owl:someValuesFrom :Symptom
                         ] .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#exacerbates
:exacerbates rdf:type owl:ObjectProperty ;
             rdfs:domain :Trigger ;
             rdfs:range [ rdf:type owl:Restriction ;
                          owl:onProperty :exacerbates ;
                          owl:someValuesFrom :MentalHealthCondition
                        ] .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#fromDrugClass
:fromDrugClass rdf:type owl:ObjectProperty ;
               rdfs:domain :PharmacologicalTreatment ;
               rdfs:range [ rdf:type owl:Restriction ;
                            owl:onProperty :fromDrugClass ;
                            owl:someValuesFrom :Drugs
                          ] .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#hasSideEffects
:hasSideEffects rdf:type owl:ObjectProperty ;
                rdfs:domain :Drugs ,
                            :SideEffect .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#increasesRiskOf
:increasesRiskOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :MentalHealthCondition ,
                             :Symptom .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isDrivenBy
:isDrivenBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Symptom ,
                        :Trigger .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isRecommendedFor
:isRecommendedFor rdf:type owl:ObjectProperty ;
                  rdfs:domain :CopingStrategy ,
                              :MentalHealthCondition ,
                              :Treatment .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isRelatedToDisorder
:isRelatedToDisorder rdf:type owl:ObjectProperty ;
                     rdfs:domain :DiagnosticCriterion ,
                                 :MentalHealthCondition .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isRelatedToSymptom
:isRelatedToSymptom rdf:type owl:ObjectProperty ;
                    rdfs:domain :DiagnosticCriterion ,
                                :Symptom .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isStageOf
:isStageOf rdf:type owl:ObjectProperty ;
           rdfs:domain :MentalHealthCondition ,
                       :SeverityLevel .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isUsedForDisorder
:isUsedForDisorder rdf:type owl:ObjectProperty ;
                   rdfs:domain :Drugs ,
                               :MentalHealthCondition .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#manifestsIn
:manifestsIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Symptom ;
             rdfs:range :MentalHealthCondition .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#mitigatesTrigger
:mitigatesTrigger rdf:type owl:ObjectProperty ;
                  rdfs:domain :CopingStrategy ;
                  rdfs:range :Trigger .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#hasDescription
:hasDescription rdf:type owl:DatatypeProperty ;
                rdfs:domain :CopingStrategy ,
                            :DiagnosticCriterion ,
                            :MentalHealthCondition ,
                            :SeverityLevel ,
                            :Symptom ,
                            :Treatment ,
                            :Trigger ;
                rdfs:range xsd:string .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#hasMechanismDescription
:hasMechanismDescription rdf:type owl:DatatypeProperty ;
                         rdfs:domain :CopingStrategy .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#hasMinimumDuration
:hasMinimumDuration rdf:type owl:DatatypeProperty ;
                    rdfs:domain :DiagnosticCriterion ;
                    rdfs:range xsd:int .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isStructuredAndGoalOriented
:isStructuredAndGoalOriented rdf:type owl:DatatypeProperty ;
                             rdfs:domain :PsychotherapyApproach ;
                             rdfs:range xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#isSuitableForAI
:isSuitableForAI rdf:type owl:DatatypeProperty ;
                 rdfs:domain :SeverityLevel ;
                 rdfs:range xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#requiresTapering
:requiresTapering rdf:type owl:DatatypeProperty ;
                  rdfs:domain :PharmacologicalTreatment ;
                  rdfs:range xsd:boolean .


