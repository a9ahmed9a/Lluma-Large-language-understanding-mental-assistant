:Trigger rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Abnormal_Dreams
:Abnormal_Dreams rdf:type owl:NamedIndividual ,
                          :SideEffect ;
                 :hasName "Abnormal Dreams" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Acceptance
:Acceptance rdf:type owl:NamedIndividual ,
                     :CognitiveCoping ;
            :alleviatesSymptom :Excessive_Worry ,
                               :Irritability ;
            :isRecommendedFor :Generalized_Anxiety_Disorder ;
            :hasDescription "This involves allowing inner thoughts and feelings to occur without trying to change or ignore them, fostering a non-judgmental relationship with internal experiences." ;
            :hasMechanismDescription "It reduces the struggle against unwanted emotions, which paradoxically can lessen their intensity." ;
            :hasName "Acceptance" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Acceptance_and_Commitment_Therapy
:Acceptance_and_Commitment_Therapy rdf:type owl:NamedIndividual ,
                                            :PsychotherapyApproach ;
                                   :isRecommendedFor :Generalized_Anxiety_Disorder ,
                                                     :Major_Depressive_Disorder ;
                                   :hasDescription "ACT belongs to the third wave of cognitive-behavioral therapies. It emphasizes psychological flexibility, which is the ability to embrace thoughts, feelings, and experiences without judgment or avoidance, to live a life aligned with personal values. It utilizes six interconnected processes: acceptance, cognitive defusion, being present (mindfulness), self-as-context, values, and committed action." ;
                                   :hasName "Acceptance and Commitment Therapy (ACT)" ;
                                   :isStructuredAndGoalOriented "false"^^xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Anhedonia
:Anhedonia rdf:type owl:NamedIndividual ,
                    :Symptom ;
           :manifestsIn :Major_Depressive_Disorder ;
           :hasDescription "A markedly diminished interest or pleasure in all or almost all activities that were previously enjoyable, occurring most of the day, nearly every day." .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Atypical_Antidepressants
:Atypical_Antidepressants rdf:type owl:NamedIndividual ,
                                   :PharmacologicalTreatment ;
                          :alleviatesSymptom :Depressed_Mood ;
                          :isRecommendedFor :Major_Depressive_Disorder ;
                          :hasDescription "Lumateperone predominantly functions as a 5-HT2a antagonist, with potential SERT inhibition at higher doses and indirect D1-mediated effects on prefrontal cortical neurotransmission." ;
                          :hasName "Atypical Antidepressants" ;
                          :requiresTapering "false"^^xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Benzodiazepines
:Benzodiazepines rdf:type owl:NamedIndividual ,
                          :PharmacologicalTreatment ;
                 :alleviatesSymptom :Sleep_Disturbance ;
                 :isRecommendedFor :Excessive_Worry ,
                                   :Generalized_Anxiety_Disorder ,
                                   :Insomnia_Disorder ;
                 :hasDescription "Act as GABA-A receptor agonists, enhancing the effect of the inhibitory neurotransmitter GABA, leading to sedative, anxiolytic, and muscle relaxant effects." ;
                 :hasName "Benzodiazepines" ;
                 :requiresTapering "false"^^xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Brain_Chemistry_Differences
:Brain_Chemistry_Differences rdf:type owl:NamedIndividual ,
                                      :PhysiologicalTrigger ;
                             :canInitiate :Generalized_Anxiety_Disorder ;
                             :hasDescription "Differences in brain chemistry and function, including low serotonin system activity and elevated noradrenergic system activity, are believed to contribute to GAD. Altered neurotransmission (e.g., serotonin, norepinephrine, dopamine, GABA, glutamate) is implicated in MDD." ;
                             :hasName "Brain Chemistry Differences" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Cognitive_Behavioral_Therapy
:Cognitive_Behavioral_Therapy rdf:type owl:NamedIndividual ,
                                       :PsychotherapyApproach ;
                              :alleviatesSymptom :Depressed_Mood ,
                                                 :Excessive_Worry ;
                              :isRecommendedFor :Generalized_Anxiety_Disorder ,
                                                :Major_Depressive_Disorder ;
                              :hasDescription "CBT is a structured, goal-oriented talk therapy based on the principle that thoughts, feelings, and behaviors are interconnected. It aims to identify and change negative or distorted thought patterns (cognitive restructuring) and maladaptive behaviors, thereby reducing symptoms and distress. It emphasizes present and future cognitions and behaviors." ;
                              :hasName "Cognitive Behavioral Therapy (CBT)" ;
                              :isStructuredAndGoalOriented "true"^^xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Complex_Sleep_Behaviors
:Complex_Sleep_Behaviors rdf:type owl:NamedIndividual ,
                                  :SideEffect ;
                         :hasName "Complex Sleep Behaviors" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Conditioned_Arousal
:Conditioned_Arousal rdf:type owl:NamedIndividual ,
                              :EnvironmentalTrigger ;
                     :canInitiate :Insomnia_Disorder ;
                     :causeSymptom :Sleep_Disturbance ;
                     :hasDescription "The bedroom itself can become linked with wakefulness or negative emotions, where past struggles to sleep make the bed a cue for arousal rather than rest." ;
                     :hasName "Conditioned Arousal" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Depressed_Mood
:Depressed_Mood rdf:type owl:NamedIndividual ,
                         :Symptom ;
                :manifestsIn :Major_Depressive_Disorder ;
                :hasDescription "A persistently low, sad, empty, or hopeless mood that is present most of the day, nearly every day." .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Depressed_Mood_Diag
:Depressed_Mood_Diag rdf:type owl:NamedIndividual ,
                              :DiagnosticCriterion ;
                     :isRelatedToDisorder :Major_Depressive_Disorder ;
                     :isRelatedToSymptom :Depressed_Mood ;
                     :hasDescription "Most of the day, nearly every day; may be subjective (e.g., feels sad, empty, hopeless) or observed by others (e.g., appears tearful). In children and adolescents, this can be an irritable mood." ;
                     :hasMinimumDuration 14 ;
                     :hasName "Depressed Mood" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dialectical_Behavior_Therapy
:Dialectical_Behavior_Therapy rdf:type owl:NamedIndividual ,
                                       :PsychotherapyApproach ;
                              :alleviatesSymptom :Depressed_Mood ;
                              :isRecommendedFor :Major_Depressive_Disorder ;
                              :hasDescription "DBT combines cognitive-behavioral techniques with mindfulness strategies. It is structured around four key components: mindfulness, distress tolerance, emotion regulation, and interpersonal effectiveness. DBT emphasizes balancing acceptance of current reality with the need for change ('dialectics')" ;
                              :hasName "Dialectical Behavior Therapy (DBT)" ;
                              :isStructuredAndGoalOriented "true"^^xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Diarrhea
:Diarrhea rdf:type owl:NamedIndividual ,
                   :SideEffect ;
          :hasName "Diarrhea" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Diet
:Diet rdf:type owl:NamedIndividual ,
               :LifestyleIntervention ;
      :alleviatesSymptom :Depressed_Mood ,
                         :Irritability ;
      :hasDescription "Dietary patterns influence mental health through various mechanisms, including effects on glycaemia (blood glucose levels), immune activation/inflammation, and the gut microbiome-brain axis. Healthy diets (e.g., Mediterranean diet) are associated with reduced inflammation and a healthier gut microbiome, which can positively influence mood and reduce depression risk. Conversely, high-glycemic and pro-inflammatory diets (e.g., high in refined carbohydrates, trans fats) can lead to rapid blood glucose fluctuations and increased inflammation, potentially worsening mood and anxiety." ;
      :hasName "Diet" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Difficulty_Controlling_Worry
:Difficulty_Controlling_Worry rdf:type owl:NamedIndividual ,
                                       :PsychologicalTrigger ;
                              :causeSymptom :Excessive_Worry ;
                              :exacerbates :Generalized_Anxiety_Disorder ;
                              :hasDescription "The inability to stop or control worrying fuels the anxious state." ;
                              :hasName "Difficulty Controlling Worry" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Difficulty_Controlling_Worry_diag
:Difficulty_Controlling_Worry_diag rdf:type owl:NamedIndividual ,
                                            :DiagnosticCriterion ;
                                   :isRelatedToDisorder :Generalized_Anxiety_Disorder ;
                                   :isRelatedToSymptom :Difficulty_Controlling_Worry ;
                                   :hasDescription "Individuals with GAD find it difficult to control this worry." ;
                                   :hasName "Difficulty Controlling Worry" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Difficulty_Maintaining_Sleep
:Difficulty_Maintaining_Sleep rdf:type owl:NamedIndividual ,
                                       :DiagnosticCriterion ;
                              :isRelatedToDisorder :Insomnia_Disorder ;
                              :isRelatedToSymptom :Difficulty_Maintaining_Sleep ;
                              :hasDescription "Characterized by frequent awakenings during the night or problems returning to sleep after waking." ;
                              :hasMinimumDuration 90 ;
                              :hasName "Difficulty Maintaining Sleep" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Difficulty_with_Sleep_Initiation
:Difficulty_with_Sleep_Initiation rdf:type owl:NamedIndividual ,
                                           :DiagnosticCriterion ;
                                  :isRelatedToDisorder :Insomnia_Disorder ;
                                  :isRelatedToSymptom :Difficulty_with_Sleep_Initiation ;
                                  :hasDescription "Consistent trouble falling asleep at the beginning of the night. In children, this may manifest as difficulty initiating sleep without caregiver intervention." ;
                                  :hasMinimumDuration 90 ;
                                  :hasName "Difficulty with Sleep Initiation" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dissatisfaction_with_Sleep_Quantity_or_Quality
:Dissatisfaction_with_Sleep_Quantity_or_Quality rdf:type owl:NamedIndividual ,
                                                         :DiagnosticCriterion ;
                                                :isRelatedToDisorder :Insomnia_Disorder ;
                                                :isRelatedToSymptom <http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dissatisfaction_with_Sleep_Quality/Quantity:> ;
                                                :hasDescription "Persistent dissatisfaction with sleep quantity or quality." ;
                                                :hasMinimumDuration 90 ;
                                                :hasName "Dissatisfaction with Sleep Quantity or Quality" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dissociation
:Dissociation rdf:type owl:NamedIndividual ,
                       :SideEffect ;
              :hasName "Dissociation" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dizziness
:Dizziness rdf:type owl:NamedIndividual ,
                    :SideEffect ;
           :hasName "Dizziness" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Drowsiness
:Drowsiness rdf:type owl:NamedIndividual ,
                     :SideEffect ;
            :hasName "Drowsiness" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dry_Mouth
:Dry_Mouth rdf:type owl:NamedIndividual ,
                    :SideEffect ;
           :hasName "Dry Mouth" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Dual_Orexin_Receptor_Antagonists
:Dual_Orexin_Receptor_Antagonists rdf:type owl:NamedIndividual ,
                                           :PharmacologicalTreatment ;
                                  :alleviatesSymptom :Sleep_Disturbance ;
                                  :isRecommendedFor :Insomnia_Disorder ;
                                  :hasDescription "Selectively block the binding of wake-promoting orexin neuropeptides A and B to their receptors (OX1R and OX2R), thereby suppressing wakefulness and facilitating sleep." ;
                                  :hasName "Dual Orexin Receptor Antagonists (DORAS)" ;
                                  :requiresTapering "false"^^xsd:boolean .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Duloxetine
:Duloxetine rdf:type owl:NamedIndividual ,
                     :Drugs ;
            :fromDrugClass :SNRI ;
            :hasSideEffects :Diarrhea ,
                            :Dry_Mouth ,
                            :Headache ,
                            :Nausea ;
            :isUsedForDisorder :Generalized_Anxiety_Disorder ,
                               :Major_Depressive_Disorder ;
            :hasDescription "Inhibits the reuptake of both serotonin (5-HT) and norepinephrine (NE), and to some extent dopamine (DA), thereby increasing their concentrations in the brain" ;
            :hasName "Duloxetine" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Escitalopram
:Escitalopram rdf:type owl:NamedIndividual ,
                       :Drugs ;
              :fromDrugClass :SSRI ;
              :hasSideEffects :Diarrhea ,
                              :Dry_Mouth ,
                              :Headache ,
                              :Nausea ;
              :isUsedForDisorder :Generalized_Anxiety_Disorder ,
                                 :Major_Depressive_Disorder ;
              :hasDescription "Selectively inhibits the reuptake of serotonin (5-HT) in the brain, increasing its availability in the synaptic cleft, which is believed to modulate mood and anxiety" ;
              :hasName "Escitalopram" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Eszopiclone
:Eszopiclone rdf:type owl:NamedIndividual ,
                      :Drugs ;
             :fromDrugClass :Nonbenzodiazepines ;
             :hasSideEffects :Complex_Sleep_Behaviors ,
                             :Dizziness ,
                             :Drowsiness ,
                             :Headache ,
                             :Nausea ,
                             :Somnolence ;
             :isUsedForDisorder :Insomnia_Disorder ;
             :hasDescription "Selectively binds to specific subunits of the GABA-A receptor complex, enhancing GABA's inhibitory effects to promote sleep" ;
             :hasName "Eszopiclone" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Excessive_Anxiety_and_Apprehensive_Expectation
:Excessive_Anxiety_and_Apprehensive_Expectation rdf:type owl:NamedIndividual ,
                                                         :DiagnosticCriterion ;
                                                :isRelatedToDisorder :Generalized_Anxiety_Disorder ;
                                                :isRelatedToSymptom :Excessive_Worry ;
                                                :hasDescription "Occurring more days than not for at least six months, concerning a variety of events or activities such as work or school performance." ;
                                                :hasMinimumDuration 180 ;
                                                :hasName "Excessive Anxiety and Apprehensive Expectation" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Excessive_Worry
:Excessive_Worry rdf:type owl:NamedIndividual ,
                          :Symptom ;
                 :isDrivenBy <http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Overthinking/Catastrophizing> ;
                 :manifestsIn :Generalized_Anxiety_Disorder ;
                 :hasDescription "Persistent and uncontrollable worry about various everyday aspects of life." .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Excessive_and_Unrealistic_Worry
:Excessive_and_Unrealistic_Worry rdf:type owl:NamedIndividual ,
                                          :PsychologicalTrigger ;
                                 :canInitiate :Generalized_Anxiety_Disorder ;
                                 :causeSymptom :Excessive_Worry ;
                                 :hasDescription "The core feature of GAD itself, where persistent and uncontrollable worry about everyday things acts as a primary psychological trigger." ;
                                 :hasName "Excessive and Unrealistic Worry" .


###  http://www.semanticweb.org/ahmed/ontologies/2025/5/OntoIDA#Exercise
:Exercise rdf:type owl:NamedIndividual ,
                   :LifestyleIntervention ;
          :alleviatesSymptom :Depressed_Mood ,
                             :Excessive_Worry ,
                             :Fatigue ,
                             :Sleep_Disturbance ;
          :isRecommendedFor :Generalized_Anxiety_Disorder ,
                            :Major_Depressive_Disorder ;
          :hasDescription "Physical activity reduces symptoms of depression and anxiety through a combination of biological and psychological pathways. Biologically, exercise increases the production of feel-good endorphins and other brain chemicals, reduces inflammatory and oxidative stress markers, promotes neurogenesis (growth of new brain cells), and activates the endocannabinoid system. Psychologically, it can reduce anxiety sensitivity, improve self-efficacy, boost self-confidence, provide a distraction from worries, and offer opportunities for social interaction." ;
          :hasName "Exercise" .


